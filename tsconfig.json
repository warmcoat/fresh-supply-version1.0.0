{
	"compilerOptions": {
		"noUnusedLocals": false /* 检查未使用的变量 */,
		"noUnusedParameters": false /* 检查未使用的函数参数 */,
		"jsx": "preserve",
		//配置 @
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/*"]
		},
		"skipLibCheck": true
	},
	"include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
	"exclude": ["node_modules"],
	"files": [],
	"references": [
		{
			"path": "./tsconfig.app.json"
		},
		{
			"path": "./tsconfig.node.json"
		}
	]
}
